extends layout

block content
  .w3-container
    .w3-card-4
      header.w3-container.w3-khaki.w3-margin-top
        h3 Lista de Periodos

        .grid-container
            div(class="grid-container")
                each prd in lista
                    div(class="grid-item")  
                        a(href="/periodo/"+prd) #{prd}
