extends layout

block content
  .w3-container
    .w3-card-4
      header.w3-container.w3-khaki.w3-margin-top
        h3 Lista de Elementos

        .grid-container
            div(class="grid-container")
                each e in lista
                    div(class="grid-item")  
                        a(href="/elemento/"+e) #{e}
